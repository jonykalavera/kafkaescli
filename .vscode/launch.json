{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "py2puml domain",
            "type": "python",
            "request": "launch",
            "module": "py2puml",
            "justMyCode": false,
            "args": [
                "kafkaescli/domain/",
                "kafkaescli.domain"
            ]
        },
        {
            "name": "run server",
            "type": "python",
            "request": "launch",
            "module": "kafkaescli",
            "justMyCode": true,
            "args": [
                "--config-file-path",
                "examples/config.json",
                "runserver"
            ]
        },
        {
            "name": "produce json",
            "type": "python",
            "request": "launch",
            "module": "kafkaescli",
            "justMyCode": true,
            "args": [
                "--middleware-classes",
                "examples.json.JSONMiddleware",
                "produce",
                "json",
                "\"{\\\"foo\\\": \\\"bar\\\"}\""
            ]
        },
        {
            "name": "consume json",
            "type": "python",
            "request": "launch",
            "module": "kafkaescli",
            "justMyCode": true,
            "args": [
                "--middleware-classes",
                "examples.json.JSONMiddleware",
                "consume",
                "--metadata",
                "json",
            ]
        },
        {
            "name": "produce avro",
            "type": "python",
            "request": "launch",
            "module": "kafkaescli",
            "justMyCode": true,
            "args": [
                "--middleware-classes",
                "examples.avro.AvroMiddleware",
                "produce",
                "avro",
                "\"{\\\"Name\\\": \\\"Carnada\\\", \\\"Age\\\": 5 }\"",
            ]
        },
        {
            "name": "consume avro",
            "type": "python",
            "request": "launch",
            "module": "kafkaescli",
            "justMyCode": true,
            "args": [
                "--middleware-classes",
                "examples.avro.AvroMiddleware",
                "consume",
                "--metadata",
                "avro",
            ]
        },
        {
            "name": "consume hello",
            "type": "python",
            "request": "launch",
            "module": "kafkaescli",
            "justMyCode": false,
            "args": [
                "consume",
                "hello"
            ]
        },
        {
            "name": "consume possible_anomalies",
            "type": "python",
            "request": "launch",
            "module": "kafkaescli",
            "justMyCode": false,
            "args": [
                "--bootstrap-servers",
                "localhost:29092",
                "consume",
                "--auto-offset-reset",
                "earliest",
                "possible_anomalies"
            ]
        },
        {
            "name": "consume possible_anomalies",
            "type": "python",
            "request": "launch",
            "module": "kafkaescli",
            "justMyCode": false,
            "args": [
                "--bootstrap-servers",
                "localhost:29092",
                "consume",
                "--auto-offset-reset",
                "earliest",
                "possible_anomalies"
            ]
        },
        {
            "name": "produce hello world",
            "type": "python",
            "request": "launch",
            "module": "kafkaescli",
            "justMyCode": true,
            "args": [
                "produce",
                "hello",
                "world"
            ]
        }
    ]
}