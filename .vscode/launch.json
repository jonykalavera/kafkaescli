{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "produce json",
            "type": "python",
            "request": "launch",
            "module": "kafkescli",
            "justMyCode": true,
            "args": [
                "--middleware-classes",
                "examples.json.JSONMiddleware",
                "produce",
                "json",
                "\"{\\\"foo\\\": \\\"bar\\\"}\""
            ]
        },
        {
            "name": "consume json",
            "type": "python",
            "request": "launch",
            "module": "kafkescli",
            "justMyCode": true,
            "args": [
                "--middleware-classes",
                "examples.json.JSONMiddleware",
                "consume",
                "--metadata",
                "json",
            ]
        },
