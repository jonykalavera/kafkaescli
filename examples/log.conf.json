{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "standard": {
      "format": "%(asctime)s - %(levelname)s - %(name)s - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "standard",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "uvicorn": {
      "error": {
        "propagate": true
      },
      "level": "ERROR"
    },
    "aiokafka": {
        "level": "ERROR"
    }
  },
  "root": {
    "level": "INFO",
    "handlers": ["console"],
    "propagate": "no"
  }
}
